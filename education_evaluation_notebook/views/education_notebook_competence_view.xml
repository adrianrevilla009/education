<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
	<!-- FORM COMPETENCIA -->
	<record id="education_notebook_competence_view_form"
		model="ir.ui.view">
		<field name="model">education.competence</field>
		<field name="arch" type="xml">
		<sheet>
			<form string="Competence">
					<group string="Competence fields">
						<field name="name" />
						<field name="eval_mode" />
						<field name="evaluation_check" />
						<field name="global_check" />
					</group>
			</form>
	      </sheet>
		</field>
	</record>
	<!-- TREE COMPETENCIA -->
	<record id="education_notebook_competence_view_tree"
		model="ir.ui.view">
		<field name="model">education.competence</field>
		<field name="arch" type="xml">
			<tree string="Competences">
				<field name="name" />
				<field name="eval_mode" />
				<field name="evaluation_check" />
                <field name="global_check" />
			</tree>
		</field>
	</record>
</odoo>

